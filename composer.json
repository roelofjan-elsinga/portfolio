{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^5.8.0",
        "eventviva/php-image-resize": "1.5.*",
        "erusev/parsedown": "^1.7",
        "laravelcollective/html": "^5.8.0",
        "symfony/yaml": "^4.2",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-curl": "*",
        "ext-zip": "*",
        "roelofjan-elsinga/content-to-html-parser": "^0.1.0",
        "roelofjan-elsinga/flat-file-cms-gui": "^0.11.0",
        "rbdwllr/reallysimplejwt": "^2.0",
        "roelofjan-elsinga/flat-file-cms": "^0.10.4",
        "roelofjan-elsinga/flat-file-cms-publish": "^0.2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~7.0",
        "filp/whoops": "~2.0",
        "nunomaduro/collision": "^2.0",
        "symfony/dom-crawler": "~4.0",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Main\\": "app/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "php artisan flatfilecms:set-permissions"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
